<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:TMDBApp"
             xmlns:base="clr-namespace:TMDBApp.Core.ViewModels.Base;assembly=TMDBApp.Core"
             xmlns:views="clr-namespace:TMDBApp.Core.Views;assembly=TMDBApp.Core"
             x:Class="TMDBApp.Core.Views.MainPage"
			 base:ViewModelLocator.AutoWireViewModel="true">

    <TabbedPage.Title>
        <OnPlatform x:TypeArguments="x:String">
            <On Platform="iOS" Value="TBMD App" />
        </OnPlatform>
    </TabbedPage.Title>
    <views:MovieListView Icon="ic_movie_creation_18pt" x:Name="MovieListView">
    </views:MovieListView>

</TabbedPage>
